upstream web1 {
      server localhost:8443;
}

server {

    server_name ecom.boldviewers.com;

    location / {
   
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
    proxy_set_header Host $host;
    proxy_set_header X-Forwarded-Proto $scheme;
    proxy_pass http://web1;
    }
    listen 443 ssl http2;
    listen [::]:443 ssl http2;
    ssl_certificate /etc/letsencrypt/live/ecom.boldviewers.com/fullchain.pem; # managed by Certbot
    ssl_certificate_key /etc/letsencrypt/live/ecom.boldviewers.com/privkey.pem; # managed by Certbot

}

# Virtual Host configuration for example.com
#
# You can move that to a different file under sites-available/ and symlink that


server {
    if ($host = prod-api.appinvento.io) {
        return 301 https://$host$request_uri;
    } # managed by Certbot



server_name prod-api.appinvento.io;

	listen 80;
    listen [::]:80 default_server;
    return 404; # managed by Certbot


}

